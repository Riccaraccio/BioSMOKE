Dictionary BioSMOKE
{
	@KineticsFolder 			   ..\kinetics\Polimi_2003\kinetics_compiled;
	
	@Type		               	   TG_Analysis;  //Total_Analysis 

	@TGA			               TG_Analysis;

	@Particle			           Total_Analysis;

	@InletGasStatus                inlet-gas-mixture;
	
	@InletSolidStatus              inlet-solid-mixture;		
}

Dictionary inlet-gas-mixture
{
	@Temperature   	             743     K ; 
	@Pressure      	             101325  Pa;
	@MassFractions		         N2      1;
}

Dictionary inlet-solid-mixture
{
	@Temperature   	            300     K ;    
	@Density		            850     Kg/m3; 	   			 
	@Pressure      	            101325  Pa;
	@MassFractions		        CELL	0.4807
								XYHW	0.2611 
								LIGO	0.1325 
								LIGH	0.0957
								LIGC	0.0214
								ASH	    0.0086;
}
  
Dictionary TG_Analysis
{
	@HeatingRate      	        10  K/min;

	@TotalSimulationTime		        10000  s;
}

Dictionary Total_Analysis
{
	@Type			        Non_Isothermal;

	@ConstantVolume			true;

	@Porosity               0.4;            
	
	@Nlayer	                10 ;

	@Radius                 12.7 mm;
	
	@TotalSimulationTime	1200 s;

	@Darcy_number		    1e-14 ;
	
	@Lambda			        0.1937 W/m/K;
	
	@hext			        1e5 W/m2/K;  
}